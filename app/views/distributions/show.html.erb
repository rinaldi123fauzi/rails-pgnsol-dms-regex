<div class="page-header">
  <%= link_to distributions_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_distribution_path(@distribution), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show distribution</h1>
</div>

<dl class="dl-horizontal">
  <dt>Document:</dt>
  <dd><%= @distribution.document_id %></dd>

  <dt>Internal audit:</dt>
  <dd><%= @distribution.internal_audit_id %></dd>

  <dt>Sender:</dt>
  <dd><%= @distribution.sender %></dd>

  <dt>Receiver:</dt>
  <dd><%= @distribution.receiver %></dd>

  <dt>Date:</dt>
  <dd><%= @distribution.date %></dd>

</dl>
