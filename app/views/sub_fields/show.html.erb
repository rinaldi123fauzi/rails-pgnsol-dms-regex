<div class="page-header">
  <%= link_to sub_fields_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_sub_field_path(@sub_field), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show sub_field</h1>
</div>

<dl class="dl-horizontal">
  <dt>Field:</dt>
  <dd><%= @sub_field.field_id %></dd>

  <dt>Description:</dt>
  <dd><%= @sub_field.description_sub_field %></dd>

</dl>
