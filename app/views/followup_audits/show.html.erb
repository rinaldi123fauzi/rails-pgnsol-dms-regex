<div class="page-header">
  <%= link_to followup_audits_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_followup_audit_path(@followup_audit), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show followup_audit</h1>
</div>

<dl class="dl-horizontal">
  <dt>Internal audit:</dt>
  <dd><%= @followup_audit.internal_audit %></dd>

  <dt>Notes:</dt>
  <dd><%= @followup_audit.notes %></dd>

  <dt>Descriptions:</dt>
  <dd><%= @followup_audit.descriptions %></dd>

  <dt>File upload:</dt>
  <dd><%= @followup_audit.file_upload %></dd>

</dl>
