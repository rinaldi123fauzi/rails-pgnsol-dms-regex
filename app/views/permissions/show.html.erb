<!-- MAIN CONTENT -->
<div id="content">
<div class="page-header">
  <%= link_to permissions_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_permission_path(@permission), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show permission</h1>
</div>

<dl class="dl-horizontal">
  <dt>Member:</dt>
  <dd><%= @permission.member_id %></dd>

  <dt>Role:</dt>
  <dd><%= @permission.role_id %></dd>

</dl>
</div>
